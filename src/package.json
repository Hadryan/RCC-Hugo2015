  "dependencies": {
    "basscss": "^8.0.1",
    "basscss-align": "latest",
    "basscss-border": "latest",
    "basscss-flexbox": "latest",
    "basscss-grid": "latest",
    "basscss-hide": "latest",
    "basscss-layout": "latest",
    "basscss-margin": "latest",
    "basscss-padding": "latest",
    "basscss-position": "latest",
    "basscss-type-scale": "latest",
    "basscss-typography": "latest",
    "basscss-basic": "latest",
    "basscss-btn": "latest",
    "basscss-btn-primary": "latest",
    "basscss-btn-outline": "latest",
    "basscss-btn-sizes": "latest",
    "basscss-forms": "latest",
    "basscss-responsive-margin": "latest",
    "basscss-responsive-padding": "latest",
    "basscss-colors": "latest",
    "basscss-background-colors": "latest",
    "basscss-background-images": "latest",
    "basscss-border-colors": "latest",
    "basscss-darken": "latest",
    "basscss-lighten": "latest",
    "basscss-input-range": "latest",
    "basscss-progress": "latest",
    "basscss-all": "latest",
    "basscss-media-object": "latest",
    "cssrecipes-tooltip": "latest"
  },
  "devDependencies": {
    "css-mqpacker": "^4.0.0",
    "cssnano": "^3.5.2",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-concat": "^2.6.0",
    "gulp-postcss": "^6.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "http-server": "^0.9.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^7.1.3",
    "postcss-reporter": "^1.3.3"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    }
  }
}
