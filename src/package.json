{
  "name": "RCC",
  "version": "2.1.1",
  "description": "Rick Cogley Central.",
  "author": {
    "name": "Rick Cogley",
    "email": "rick@cogley.info",
    "url": "http://rick.cogley.info"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickCogley/RCC-Hugo2015"
  },
  "keywords": [
    "Basscss",
    "Bassplate",
    "Jayden Seric Barebones",
    "Boilerplate",
    "CSS",
    "OOCSS",
    "cssnext",
    "postcss"
  ],
  "bugs": {
    "url": "https://github.com/RickCogley/RCC-Hugo2015/issues"
  },
  "private": true,
  "scripts": {
    "lint": "eslint . || true",
    "task": "gulp",
